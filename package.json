{
  "name": "cypress-practice",
  "version": "1.0.0",
  "description": "Cypress practice",
  "main": "index.js",
  "scripts": {
    "pretest-windows": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports",
    "pretest": "rm -rf cypress/reports && mkdir cypress/reports",
    "lint": "eslint cypress/e2e && eslint cypress/support/pageObjects",
    "lint-fix": "npm run lint -- --fix",
    "test": "npm run pretest && npx cypress run",
    "test-windows": "npm run pretest-windows && npx cypress run --browser chrome",
    "ci-test": "npm run pretest && npx cypress run --reporter-options=reportFilename=index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaffyRod/cypress-practice.git"
  },
  "author": "Raffy Rodriguez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RaffyRod/cypress-practice/issues"
  },
  "homepage": "https://github.com/RaffyRod/cypress-practice#readme",
  "devDependencies": {
    "cypress": "^12.0.1",
    "eslint": "^8.29.0"
  },
  "dependencies": {
    "@ngneat/falso": "^6.3.0",
    "cypress-mochawesome-reporter": "^3.2.3"
  }
}
